{
  "containerDefinitions": [
    {
      "name": "tis-trainee-details",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-trainee-details:latest",
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "/tis/trainee/${environment}/db/host"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "/tis/trainee/${environment}/db/port"
        },
        {
          "name": "DB_USER",
          "valueFrom": "/tis/trainee/${environment}/db/username"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "/tis/trainee/${environment}/db/password"
        },
        {
          "name": "SENTRY_DSN",
          "valueFrom": "tis-trainee-details-sentry-dsn"
        },
        {
          "name": "EVENT_QUEUE_URL",
          "valueFrom": "/tis/trainee/${environment}/queue-url/profile-created"
        },
        {
          "name": "SIGNATURE_SECRET_KEY",
          "valueFrom": "/tis/trainee/${environment}/signature/secret-key"
        },
        {
          "name": "DSP_CLIENT_ID",
          "valueFrom": "/tis/trainee/${environment}/dsp/client-id"
        },
        {
          "name": "DSP_CLIENT_SECRET",
          "valueFrom": "/tis/trainee/${environment}/dsp/client-secret"
        },
        {
          "name": "DSP_REDIRECT_URI",
          "valueFrom": "/tis/trainee/${environment}/dsp/redirect-uri"
        },
        {
          "name": "DSP_PAR_ENDPOINT",
          "valueFrom": "/tis/trainee/${environment}/dsp/par-endpoint"
        },
        {
          "name": "DSP_AUTHORIZE_ENDPOINT",
          "valueFrom": "/tis/trainee/${environment}/dsp/authorize-endpoint"
        },
        {
          "name": "DSP_TOKEN_ISSUER",
          "valueFrom": "/tis/trainee/${environment}/dsp/token-issuer"
        },
        {
          "name": "DSP_TOKEN_AUDIENCE",
          "valueFrom": "/tis/trainee/${environment}/dsp/token-audience"
        },
        {
          "name": "DSP_TOKEN_SIGNING_KEY",
          "valueFrom": "/tis/trainee/${environment}/dsp/token-signing-key"
        },
        {
          "name": "DSP_TOKEN_ISSUE_ENDPOINT",
          "valueFrom": "/tis/trainee/${environment}/dsp/token-issue-endpoint"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-${environment}-tis-trainee-details",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "awslogs-tis-trainee-details"
        }
      },
      "portMappings": [
        {
          "containerPort": 8203
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-trainee-details"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        },
        {
          "name": "ENVIRONMENT",
          "value": "${environment}"
        }
      ]
    }
  ],
  "family": "tis-trainee-details-${environment}",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "taskRoleArn": "tis-trainee-details_task-role_${environment}",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
